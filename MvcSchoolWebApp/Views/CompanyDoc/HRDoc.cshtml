@model MvcSchoolWebApp.Models.CompanyDocModel
@{
    ViewBag.Title = "ClassTimetable";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<section class="content">
    <!-- Info boxes -->
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div id="dvborder">
                    <div class="ibox-title ">
                        <ul id="vert_menu">
                            <li>
                                <input type="radio" id="r1" name="r">
                                <label for="r1">
                                    Corporate Documents
                                </label>
                            </li>
                            <li>
                                <input type="radio" id="r2" name="r">
                                <label for="r2">
                                    HR Documents
                                </label>
                            </li>
                        </ul>
                    </div>
                    <div class="ibox-content" id="hrdoc_div">
                        <div class="row">
                            <div class="hrdoc_wrapper">
                                <table id="hrdoc_grid"></table>
                                <div id="hrdoc_pager"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>

