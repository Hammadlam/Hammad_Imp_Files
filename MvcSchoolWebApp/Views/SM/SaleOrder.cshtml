@model MvcSchoolWebApp.Models.SMModel
@{
    ViewBag.Title = "SaleOrder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content">
    <!-- Info boxes -->
    <div class="row">

        <div class="col-lg-12">

            <div class="ibox float-e-margins">
                <div id="dvborder">
                    <div class="ibox-title ">

                        <ul id="vert_menu">
                            <li>
                                <input type="radio" id="r1" name="r">
                                <label for="r1">
                                    Sales Management
                                </label>
                            </li>
                            <li>
                                <input type="radio" id="r2" name="r">
                                <label for="r2">
                                    Sales Order
                                </label>
                            </li>
                        </ul>
                    </div>

                    <div class="ibox-content">
                        <div class="row">
                            <form method="post" role="form" id="saleorderForm" name="saleorderForm">

                                <div class="row">

                                    <div class="col-md-6">
                                        <div class="col-md-5">
                                            <label>Sales Order Type</label>
                                        </div>
                                        <div class="col-md-5">
                                            @Html.DropDownListFor(m => m.sotypeid, Model.sotype, "Sales Order Type", new { @class = "form-control input-sm m-b pull-left", @name = "txtSOTypeSO", @id = "txtSOTypeSO", @required = true })
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="col-md-5">
                                            <label>Sales Order</label>
                                        </div>
                                        <div class="col-md-4">
                                            @Html.DropDownListFor(m => m.saleorderid, Model.saleorder, "Sales Order", new { @class = "form-control input-sm m-b pull-left ", @name = "txtSaleOrderSO", @id = "txtSaleOrderSO", @required = true })
                                        </div>
                                    </div>
                                </div>


                                <div class="row">
                                    <div class="col-md-6 ">
                                        <div class="col-md-12">
                                            <button type="button"> <img src="~/Content/dbtheme/dist/img/SaveIco.png" width="20" height="20" /> Save</button>
                                            <button type="button"> <img src="~/Content/dbtheme/dist/img/dl.png" width="20" height="20" /> Edit</button>
                                            <button type="button" class=".profbtn"> <img src="~/Content/dbtheme/dist/img/Redcross.png" width="20" height="20" /> Cancel</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
