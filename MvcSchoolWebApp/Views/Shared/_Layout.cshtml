                                @{
                                    try
                                    {
                                        var user_dtl = HttpContext.Current.Session["User_Dtl"] as List<MvcSchoolWebApp.Models.Users>;
                                        if (user_dtl[0].user_image == "")
                                        {
                                            <img src="~/Content/Avatar/sajidh.jpg" class="user-image" alt="User Image">
                                        }
                                        else if (user_dtl[0].user_image != "")
                                        {
                                            <img src="@Url.Content(user_dtl[0].user_image)" class="user-image" alt="User Image">

                                        }
                                    }
                                    catch (Exception)
                                    {
                                        string url = "/dashboard";
                                        Response.Write("<script type='text/javascript'>");
                                        Response.Write("window.location.href = '" + url + "'</script>");
                                    }
                                }
                            </a>
