                                <li class="header">Notifications</li>
                                <li>
                                    <ul class="menu">
                                        @if (ViewBag.msglist != null)
                                        {
                                            foreach (var item in ViewBag.msglist)
                                            {
                                                string alldata = item.MsgId + "?" + item.Groupid + "?" + item.recordno;
                                                if (item.unread == true)
                                                {
                                                    <li style="background-color:#e5e5e5;border:1px solid #ffffff;">
                                                        <a id="MsgSubject" onclick="GetMsgid(this);" value=@alldata>
                                                            <label style="font-weight:600; color:#ca434d;">@item.senderId</label><br />
                                                            @Html.Raw(item.Subject)
                                                            <p class="pull-right msg-time"> @item.msgdate </p>
                                                        </a>
                                                    </li>
                                                }
                                                else
                                                {
                                                    <li>
                                                        <a href="/Message/viewchat" id="MsgSubject" onclick="GetMsgid(this);" value=@alldata>

                                                            <label style="font-weight:600; color:#ca434d;">@item.senderId</label><br />
                                                            @Html.Raw(item.Subject)
                                                            <p class="pull-right msg-time"> @item.msgdate <br /> @item.msgtime </p>
                                                        </a>

                                                    </li>
                                                }


                                            }
                                        }
                                    </ul>
                                </li>
